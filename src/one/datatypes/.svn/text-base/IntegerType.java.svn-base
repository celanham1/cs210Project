package one.datatypes;


/**
 * The Integer Data Type.
 */
public class IntegerType extends AbstractType{

	/**
	 * Instantiates a new integer type.
	 *
	 * @param name the name
	 * @param type the type
	 */
	public IntegerType(String name, String type) {
		super(name, type);
		byteSize = 4;
	}
	

}
