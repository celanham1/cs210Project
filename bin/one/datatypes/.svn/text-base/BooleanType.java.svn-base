package one.datatypes;

/**
 * The Boolean Data Type.
 */
public class BooleanType extends AbstractType{
	
	/**
	 * Instantiates a new boolean type.
	 *
	 * @param name the name
	 * @param type the type
	 */
	public BooleanType(String name, String type) {
		super(name, type);
		byteSize = 1;
	}
	
	
}
